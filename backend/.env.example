# Backend Environment Variables
# Duplicate this file as .env and fill in your local values.
# DO NOT COMMIT .env FILES TO VERSION CONTROL.

# Database Configuration (for local PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=wins_dev_db

# Auth0 Configuration (Development Tenant)
AUTH0_DOMAIN=your-dev-domain.auth0.com
AUTH0_CLIENT_ID=your_backend_auth0_client_id
AUTH0_CLIENT_SECRET=your_backend_auth0_client_secret
AUTH0_AUDIENCE=your_api_audience # e.g., http://localhost:3001/api

# n8n Webhook Endpoints (if backend receives webhooks)
# N8N_WEBHOOK_SECRET=your_n8n_webhook_secret

# API Server Configuration
API_PORT=3001
NODE_ENV=development

# Logging Configuration
LOG_LEVEL=debug

# Add other necessary environment variables here
# e.g., API keys for external services (use development keys)
